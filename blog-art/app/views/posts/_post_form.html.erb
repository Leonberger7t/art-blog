<%= form_for Post.new do |f| %>

<%= f.label :title %>
<%= f.text_field :title %> <br>

<%= f.label :content %>
<%= f.text_field :content %> <br>

<%= f.label :User %><br>
<%= f.collection_select :user_id, User.all, :id, :email %><br>


<%= f.label :location %><br>
<%= f.text_field :location %> <br>

<%= f.label :exhibition %><br>
<%= f.text_field :exhibition %> <br>

<%= f.file_field :image %>


  <%= f.submit %>
<% end %>
